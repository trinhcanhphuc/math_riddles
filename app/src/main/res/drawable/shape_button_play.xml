<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle"
            android:gravity="center" >
            <corners android:radius="30dp" />
            <gradient
                android:angle="45"
                android:startColor="#184D2E"
                android:centerColor="#00b33c"
                android:endColor="#a6fc93"
                android:type="linear" />
        </shape>
    </item>
</layer-list>